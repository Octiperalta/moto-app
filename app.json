    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.coderhouse.motomarket",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTENERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBfVm-gBAr1o1NM5LB1Ux74cZgToA11rac"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
